<plugin>
  <artifactId>maven-clean-plugin</artifactId>
  <version>2.5</version>
  <executions>
    <execution>
      <id>clean-jar-artifacts</id>
      <phase>install</phase>
      <goals><goal>clean</goal></goals>
      <configuration>
        <excludeDefaultDirectories>true</excludeDefaultDirectories>
        <filesets>
          <fileset>
			  <directory>target</directory>
				  <excludes>
					<exclude>*.war</exclude>
				  </excludes>
          </fileset>
        </filesets>
      </configuration>
    </execution>
  </executions>
</plugin>